{
  "presets": [
    [
      "next/babel",
      {
        "styled-jsx": {
          "plugins": ["styled-jsx-plugin-postcss"]
        }
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "~/__generated__": "./__generated__",
          "~/static": "./public/static",
          "~": "./src"
        }
      }
    ]
  ]
}
