{
  "presets": [
    [
      "next/babel",
      {
        "preset-env": {
          "modules": false
        },
        "transform-runtime": {
          "useESModules": false
        },
        "styled-jsx": {
          "plugins": ["styled-jsx-plugin-postcss",  {
            "path":
              "./postcss.config.json"
          }]
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-optional-chaining"
  ]
}
